{
	"manifest_version": 3,
	"name": "Link Sharing With Queue",
	"version": "1.0.1",
	"description": "Share page URLs and titles via email with queuing",
	"permissions": [
		"tabs",
		"storage",
		"activeTab",
		"contextMenus",
		"notifications"
	],
	"action": {
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
